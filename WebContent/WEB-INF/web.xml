    </servlet>
    <servlet-mapping>
	    <servlet-name>RXDisagreeUser</servlet-name>
	    <url-pattern>/RXDisagreeUser</url-pattern>
    </servlet-mapping>

 	<servlet> 
	    <servlet-name>RXUpdateUserInfo</servlet-name>
	    <servlet-class>com.sanghafarm.servlet.im.RXUpdateUserInfo</servlet-class>
    </servlet>
    <servlet-mapping>
	    <servlet-name>RXUpdateUserInfo</servlet-name>
	    <url-pattern>/RXUpdateUserInfo</url-pattern>
    </servlet-mapping>

 	<servlet> 
	    <servlet-name>RXUpdateUserStatus</servlet-name>
	    <servlet-class>com.sanghafarm.servlet.im.RXUpdateUserStatus</servlet-class>
    </servlet>
    <servlet-mapping>
	    <servlet-name>RXUpdateUserStatus</servlet-name>
	    <url-pattern>/RXUpdateUserStatus</url-pattern>
    </servlet-mapping>

 	<servlet> 
	    <servlet-name>RXGetUserInfo</servlet-name>
