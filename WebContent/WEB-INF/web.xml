    </servlet>
    <servlet-mapping>
	    <servlet-name>RXGetUserInfo</servlet-name>
	    <url-pattern>/RXGetUserInfo</url-pattern>
    </servlet-mapping>

  <filter>
    <filter-name>Encoding Filter</filter-name>
    <filter-class>com.efusioni.stone.common.EncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>Encoding Filter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>Pre Filter</filter-name>
    <filter-class>com.sanghafarm.common.PreFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Pre Filter</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>

</web-app>